{
  "name": "micro-analytics-cli",
  "version": "1.1.0",
  "description": "Public analytics as a Node.js microservice, no sysadmin experience required.",
  "bin": {
    "micro-analytics": "./cli.js"
  },
  "files": [
    "dist",
    "cli.js",
    "adapter-tests"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "./node_modules/.bin/async-to-gen src --out-dir dist",
    "prepublish": "npm run build",
    "dev": "NODE_ENV=development nodemon --config package.json --exec async-node src/index.js",
    "test": "jest",
    "lint": "eslint src tests adapter-tests",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/*.js' 'tests/*.js' 'adapter-tests/*.js'"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "semi": false
        }
      ]
    }
  },
  "author": "Max Stoiber <contact@mxstbr.com> (http://mxstbr.com/)",
  "license": "MIT",
  "repository": "https://github.com/mxstbr/micro-analytics",
  "dependencies": {
    "args": "^2.3.0",
    "flat-file-db": "^1.0.0",
    "micro": "6.1.0",
    "micro-analytics-adapter-flat-file-db": "^1.2.0",
    "promise": "^7.1.1",
    "shelljs": "^0.7.6",
    "sse": "0.0.6",
    "update-notifier": "^1.0.3",
    "zen-observable": "^0.4.0"
  },
  "devDependencies": {
    "async-to-gen": "^1.3.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-node6": "^11.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-prettier": "^2.0.1",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "prettier": "^1.2.2",
    "request-promise": "^4.1.1"
  },
  "execMap": {
    "js": "micro"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
